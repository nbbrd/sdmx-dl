<sources>
    <source>
        <name>BIS</name>
        <description>Bank for International Settlements</description>
        <driver>ri:sdmx21</driver>
        <endpoint>https://stats.bis.org/api/v1</endpoint>
        <property key="detailSupported" value="true"/>
        <website>https://stats.bis.org/statx/toc/LBS.html</website>
    </source>
    <source>
        <name>ECB</name>
        <description>European Central Bank</description>
        <driver>ri:sdmx21</driver>
        <dialect>ECB2020</dialect>
        <endpoint>https://sdw-wsrest.ecb.europa.eu/service</endpoint>
        <property key="detailSupported" value="true"/>
        <website>https://sdw.ecb.europa.eu</website>
    </source>
    <source>
        <name>UNDATA</name>
        <description>Data access system to UN databases</description>
        <driver>ri:sdmx21</driver>
        <endpoint>https://data.un.org/WS/rest</endpoint>
        <property key="detailSupported" value="true"/>
        <website>https://data.un.org/SdmxBrowser/start</website>
    </source>
    <source>
        <name>ISTAT</name>
        <description>Istituto Nazionale di Statistica</description>
        <driver>ri:sdmx21</driver>
        <endpoint>http://sdmx.istat.it/SDMXWS/rest</endpoint>
        <property key="detailSupported" value="true"/>
        <website>https://www.istat.it/en/analysis-and-products</website>
    </source>
    <source>
        <name>WITS</name>
        <description>World Integrated Trade Solutions</description>
        <driver>ri:sdmx21</driver>
        <endpoint>http://wits.worldbank.org/API/V1/SDMX/V21/rest</endpoint>
        <property key="trailingSlashRequired" value="true"/>
        <website>https://wits.worldbank.org</website>
    </source>
    <source>
        <name>IMF_SDMX_CENTRAL</name>
        <description>International Monetary Fund SDMX Central</description>
        <driver>ri:sdmx21</driver>
        <endpoint>https://sdmxcentral.imf.org/ws/public/sdmxapi/rest</endpoint>
        <property key="detailSupported" value="true"/>
        <website>https://data.imf.org</website>
    </source>
    <source>
        <name>INEGI</name>
        <description>Instituto Nacional de Estadistica y Geografia</description>
        <driver>ri:sdmx21</driver>
        <endpoint>https://sdmx.snieg.mx/service/Rest</endpoint>
        <property key="detailSupported" value="true"/>
        <property key="trailingSlashRequired" value="true"/>
        <website>https://sdmx.snieg.mx</website>
    </source>
    <source>
        <name>WB</name>
        <description>World Bank</description>
        <driver>ri:sdmx21</driver>
        <endpoint>https://api.worldbank.org/v2/sdmx/rest</endpoint>
        <property key="detailSupported" value="true"/>
        <property key="trailingSlashRequired" value="true"/>
        <website>https://data.worldbank.org</website>
